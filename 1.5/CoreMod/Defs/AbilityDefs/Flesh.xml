<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
<defName>VPE_VoidmassSTNL</defName>
<label>Weave Small Void Mass</label>
<description>Manipulate the fabric of the void to manifest a mass of void metal.</description>
<iconPath>UI_VoidweaverSTNL/Abilities/Mass</iconPath>
<castSound>PsychicRitual_Interrupted</castSound>
<castTime>15</castTime>
<range>6.9</range>
<targetMode>Location</targetMode>
<abilityClass>VFECore.Abilities.Ability_SpawnBuilding</abilityClass>
<needsTicking>true</needsTicking>
<chance>0</chance>
<modExtensions>
<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
<path>VPE_VoidweaverSTNL</path>
<level>1</level>
<order>2</order>
<psyfocusCost>0.10</psyfocusCost>
<entropyGain>15</entropyGain>
</li>
<li Class="VFECore.Abilities.AbilityExtension_Building">
<building>VoidmetalMassSmallSTNL</building>
</li>
<li Class="VFECore.Abilities.AbilityExtension_EffecterOnTarget">
